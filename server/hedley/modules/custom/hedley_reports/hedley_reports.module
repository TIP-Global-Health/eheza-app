/**
 * Grants access for Statistical Queries and Completion Report.
 *
 * @param string $population
 *   Selected population scope.
 *   Possible options: be 'all', 'health-center', 'demographics'.
 * @param string $main_id
 *   The ID of top level item from scope from 'health-center'/'demographics'.
 *
 * @return bool
 *   TRUE if the user has access, FALSE otherwise.
 */
function hedley_reports_menu_access($population = FALSE, $main_id = FALSE) {
  $scope_data = hedley_reports_resolve_scope_for_reports_by_current_user();
  $statistical_queries_manager_role = user_role_load_by_name('Statistical Queries Manager');
  $base_condition = user_has_role($statistical_queries_manager_role->rid) || hedley_admin_administrators_access();

  if (empty($population) || $scope_data['scope'] == HEDLEY_REPORTS_SCOPE_FULL) {
    return $base_condition;
  }

  if ($scope_data['scope'] == HEDLEY_REPORTS_SCOPE_HEALTH_CENTERS) {
    return $base_condition && ($population == 'health-center') && in_array($main_id, $scope_data['items']);
  }

  return FALSE;
}

/**
 * Implements hook_node_insert().
 */
function hedley_reports_node_insert($node) {
  // Generate initial Reports data for newly created person.
  if ($node->type == 'person') {
    hedley_reports_trigger_recalculation_for_created_person($node);
    return;
  }

  hedley_reports_trigger_recalculation_by_new_content($node);
}

/**
 * Implements hook_node_update().
 */
function hedley_reports_node_update($node) {
  if ($node->type == 'person') {
    hedley_reports_trigger_recalculation_for_edited_person($node);
  }

  hedley_reports_trigger_recalculation_by_edited_content($node);
}

